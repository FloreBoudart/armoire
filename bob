<script type="text/javascript">
    require(['jquery'], function ($) {
        const wishlistToggle = $('.js-wishlist-toggle')

        $(window).on({
            'product.liked': function (e) {
                wishlistToggle.each(function (e) {
                    const wishlistToggleCount = $(this).find('.js-wishlist-toggle-count')
                    const currentCount = parseInt(wishlistToggleCount.text())
                    let newCount = currentCount + 1
                    if(newCount < 1) newCount = 0
                    wishlistToggleCount.text(newCount)

                    if(newCount < 1) $(this).removeClass('active')
                    else $(this).addClass('active')
                })
            },
            'product.unliked': function (e) {
                wishlistToggle.each(function (e) {
                    const wishlistToggleCount = $(this).find('.js-wishlist-toggle-count')
                    const currentCount = parseInt(wishlistToggleCount.text())
                    let newCount = currentCount - 1
                    if(newCount < 1) newCount = 0
                    wishlistToggleCount.text(newCount)

                    if(newCount < 1) $(this).removeClass('active')
                    else $(this).addClass('active')
                })
            }
        })
    })
</script>
<script type="text/javascript">
    require(['jquery', 'eshop', 'malihu-custom-scrollbar-plugin', 'jquery-mousewheel'], function ($, eshop) {
        var basketTimeout;

        function initMinicartSidebar() {
            const minicart = $('.js-minicart')

            if (window.innerWidth < 768 && !minicart.hasClass('js-minicart-initialized')) {
                minicart.addClass('js-minicart-initialized')
                require(['swipe-sidebar'], function () {
                    minicart.swipeSidebar('vertical').on('hide.swipe-sidebar', function () {
                        minicart.closest('.js-minicart-dropdown').removeClass('cart-open');
                        $('html, body').removeClass('active-minicart')
                        if (history.state && history.state.type === 'overlay' && history.state.id === 'minicart') {
                            history.pushState('', document.title, window.location.pathname + window.location.search);
                        }
                    })
                })
            }
        }

        initMinicartSidebar()

        $(document).on('eshop.command', function (e, command, data) {
            var minicart = data['minicart'];

            if (data && typeof data['minicart'] != 'undefined') {
                history.state && history.state.type === 'overlay' ? history.replaceState({
                    'type': 'overlay',
                    'id': 'minicart'
                }, '', '#minicart') : history.pushState({'type': 'overlay', 'id': 'minicart'}, '', '#minicart')
                $('html, body').addClass('active-minicart')
                $('.basket')
                    .html(minicart.html)
                    .addClass('cart-open', 800)
                    .on('mouseleave', function () {
                        clearTimeout(basketTimeout);
                        $(this).removeClass('cart-open');
                        $('html, body').removeClass('active-minicart')
                        if (history.state && history.state.type === 'overlay' && history.state.id === 'minicart') {
                            history.pushState('', document.title, window.location.pathname + window.location.search);
                        }
                    });

                initMinicartSidebar()

                basketTimeout = setTimeout(function () {
                    $('.basket').mouseleave();
                }, eshop.interactiveTimeout);

                eshop.bindCommands($('.basket'));
            }

            const minicartBody = $('.js-minicart-body')
            if (minicartBody.length > 0) {
                minicartBody.mCustomScrollbar('destroy')
                minicartBody.mCustomScrollbar({
                    theme: "dark-2"
                })
            }
        });

        $(document).on('click touch', '.js-minicart-dropdown-toggle', function (e) {
            const cart_menu = $('.js-minicart-dropdown');

            if (window.innerWidth < 992) {
                e.preventDefault();

                if (cart_menu.hasClass('cart-open')) {
                    cart_menu.removeClass('cart-open');
                    $('html, body').removeClass('active-minicart')
                    if (history.state && history.state.type === 'overlay' && history.state.id === 'minicart') {
                        history.pushState('', document.title, window.location.pathname + window.location.search);
                    }
                } else {
                    cart_menu.addClass('cart-open');
                    $('html, body').addClass('active-minicart')
                    history.state && history.state.type === 'overlay' ? history.replaceState({
                        'type': 'overlay',
                        'id': 'minicart'
                    }, '', '#minicart') : history.pushState({'type': 'overlay', 'id': 'minicart'}, '', '#minicart')
                }
            }
        }).on('click touch', function (e) {
            if (window.innerWidth < 992 && $(e.target).closest('.js-minicart-dropdown').length === 0 && $('.js-minicart-dropdown').hasClass('cart-open')) {
                $('.js-minicart-dropdown').removeClass('cart-open')
                $('html, body').removeClass('active-minicart')
                if (history.state && history.state.type === 'overlay' && history.state.id === 'minicart') {
                    history.pushState('', document.title, window.location.pathname + window.location.search);
                }
            }
        })

        $(document).on('mouseover', '.js-minicart-dropdown', function () {
            if(window.innerWidth > 992) {
                $('html, body').addClass('active-minicart')
            }
        }).on('mouseleave', '.js-minicart-dropdown', function () {
            if(window.innerWidth > 992) {
                $('html, body').removeClass('active-minicart')
            }
        })

        const minicartBody = $('.js-minicart-body')
        if (minicartBody.length > 0) {
            minicartBody.mCustomScrollbar('destroy')
            minicartBody.mCustomScrollbar({
                theme: "dark-2"
            })
        }

        $(document).on('full_page_cache_updated', function (e, tag, id, htmlData) {
            if (tag === 'minicart') {
                const minicartBody = $('.js-minicart-body')
                if (minicartBody.length > 0) {
                    minicartBody.mCustomScrollbar('destroy')
                    minicartBody.mCustomScrollbar({
                        theme: "dark-2"
                    })
                }
                eshop.bindCommands($('.basket'));
            }
        })

        $(window).on('hashchange', function () {
            const minicartDropdown = $('.js-minicart-dropdown')
            if (window.location.hash !== '#minicart-dropdown' && minicartDropdown.hasClass('cart-open')) {
                minicartDropdown.removeClass('cart-open')
                $('html, body').removeClass('active-minicart')
            }
        })
    })
</script>
    <script type="text/javascript">
        require(['jquery'], function ($) {
            $(function () {
                $('.js-footer:not(.initialized)').each(function () {
                    $(this).on('click', 'h2, h3', function () {
                        if (window.innerWidth <= 768) {
                            const col = $(this).closest('.site-footer-content__col--menu')
                            const colBody = col.find('ul')

                            if (colBody.is(':hidden')) {
                                col.removeClass('collapsed')
                                colBody.slideDown(400)
                            } else {
                                col.addClass('collapsed')
                                colBody.slideUp(400)
                            }
                        }
                    })
                }).addClass('initialized')
            })
        })
    </script>
    <script type="text/javascript">
        require(['jquery'], function ($) {
            $(function () {
                $('[data-menu-item-id=261050]').addClass('v-dynamic-active');
            })
        })
    </script>
    <div id="pageview-analytics"></div><script async src="https://scripts.luigisbox.com/LBX-128956.js"></script>
<div class="country-popup-wrapper js-country-popup" role="alertdialog"></div>

<script type="text/javascript">
    require(['ipcountry'], function (ipCountry) {
        ipCountry.init({
            lang: 'fr',
            site: 'be',
            version: '1'
        });
    });
</script>
<script type="text/javascript">
    require(['views-counter'], function (viewCounter) {
        let limit = 3;

        viewCounter.setTag('page_counter_views_1');
        viewCounter.setLimit(limit);
        viewCounter.countPage(255772);

        let viewedPages = viewCounter.getPages();
        if (viewedPages.length === limit && !viewCounter.isFired()) {
            viewCounter.eventFired();

            let push_object = {
                'event': 'page_views_3_plus',
                'pagesViewed': viewedPages
            };
            dataLayer.push(push_object);
        }
    });
</script>
</body>
</html>
